<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to='tortugwt'>
	<inherits name="com.google.common.collect.Collect"/>
	<inherits name='com.google.gwt.user.User' />
	<!-- We need the JUnit module in the main module, -->
	<!-- otherwise eclipse complains (Google plugin bug?) -->
	<inherits name='com.google.gwt.junit.JUnit' />

	<entry-point class='com.sfeir.gwt.tortugwt.client.Tortugwt' />
	
	<stylesheet src="/bootstrap.min.css"/>
</module>
